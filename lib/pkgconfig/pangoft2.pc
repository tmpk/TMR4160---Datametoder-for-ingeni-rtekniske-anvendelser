prefix=/target
exec_prefix=${prefix}
libdir=${exec_prefix}/lib
includedir=${prefix}/include

Name: Pango FT2
Description: Freetype 2.0 font support for Pango
Version: 1.16.1
Requires: pango
Libs: -L${libdir} -lpangoft2-1.0 -lfontconfig -lfreetype -lz
Cflags: -I${includedir}/pango-1.0 -I${includedir}/freetype2 -I${includedir}
